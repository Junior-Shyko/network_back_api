{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "text",
      "required": true
    },
    "file": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "users_permissions_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "posts"
    },
    "like": {
      "type": "biginteger",
      "default": "0"
    },
    "heart": {
      "type": "biginteger",
      "default": "0"
    },
    "like_post": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::like.like",
      "mappedBy": "post"
    },
    "heart_post": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::heart.heart",
      "mappedBy": "post"
    }
  }
}
